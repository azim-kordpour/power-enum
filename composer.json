{
  "name": "azimkordpour/power-enum",
  "description": "This light package provides some methods to your Enum classes to make the most of them.",
  "keywords": [
    "php",
    "enum",
    "laravel"
  ],
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "AzimKordpour\\PowerEnum\\": "src/"
    },
    "files": [
      "src/Helpers/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Azim Kordpour",
      "email": "mojtaba.kordpour@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "pestphp/pest": "^2.8",
    "phpstan/phpstan": "^2.1",
    "rector/rector": "^2.0",
    "laravel/pint": "^1.21",
    "symfony/var-dumper": "^7.2"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "scripts": {
    "test": "./vendor/bin/pest",
    "refacto": "rector",
    "lint": "pint",
    "test:types": "phpstan analyse --ansi"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
